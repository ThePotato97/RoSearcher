{
  "name": "RoSearcher",
  "manifest_version": 3,
  "version": "2.0.0",
  "description": "Search through the game servers for a player!",
  "icons": {
    "128": "images/icon.png"
  },
  "permissions": [
    "scripting"
  ],
  "background": {
    "service_worker": "background.ts"
  },
  "host_permissions": [
    "https://*.roblox.com/games/"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "images/*.png",
        "styles.scss",
        "content.ts"
      ],
      "matches": [
        "https://*.roblox.com/*"
      ]
    }
  ]
}