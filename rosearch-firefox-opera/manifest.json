{
  "name": "RoSearcher",
  "manifest_version": 2,
  "version": "2.0.0",
  "description": "Search through the game servers for a player!",
  "icons": {
    "128": "images/icon.png"
  },
  "permissions": [
    "*://*.roblox.com/*",
    "*://*.rbxcdn.com/*"
  ],
  "web_accessible_resources": [
    "images/*.png",
    "content.js",
    "load.js",
    "panel.html"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.roblox.com/games/*"
      ],
      "run_at": "document_idle",
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}